    "babel-preset-env": "^1.6.1",
    "buffer": "^5.1.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
